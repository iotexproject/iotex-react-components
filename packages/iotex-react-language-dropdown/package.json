    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-react-require": "3.1.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "browser-env": "3.2.6",
    "build-changelog": "2.1.2",
    "global": "4.3.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.7.0",
    "nyc": "14.1.1",
    "onefx": "1.5.3",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "react": "16.8.6",
    "react-test-renderer": "16.8.6",
    "shader": "1.0.0",
