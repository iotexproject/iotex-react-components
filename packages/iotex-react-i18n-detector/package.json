{
  "name": "iotex-react-i18n-detector",
  "version": "2.4.3",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "tslint --fix --project tsconfig.json -t stylish",
    "watch": "gulp",
    "build": "rm -rf && tsc && webpack --config ./webpack.config.js",
    "test": "npm run cover",
    "ava": "NODE_ENV=test ava",
    "cover": "npm run lint && nyc npm run ava"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts",
      "src/**/__tests__/**"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "cobertura"
    ],
    "extension": [
      ".tsx",
      ".ts"
    ],
    "cache": true,
    "temp-directory": "./coverage/.nyc_output"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fancy-log": "1.3.3",
    "nodegit": "0.24.3",
    "nodegit-kit": "0.20.1"
  },
  "devDependencies": {
    "@types/fancy-log": "1.3.1",
    "@types/node": "12.12.35",
    "@types/nodegit": "0.24.8",
    "ava": "2.4.0",
    "awesome-typescript-loader": "5.2.1",
    "gulp": "4.0.2",
    "nyc": "14.1.1",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "6.2.0",
    "tslint-require-connnect-typing": "1.0.1",
    "typescript": "3.5.3",
    "uglifyjs-webpack-plugin": "2.1.3",
    "webpack": "4.36.1",
    "webpack-cli": "3.3.3",
    "webpack-node-externals": "1.7.2"
  },
  "gitHead": "0f8468adb54e8a6657ad5c27e6a8c8e9783d8d3b",
  "private": true
}
